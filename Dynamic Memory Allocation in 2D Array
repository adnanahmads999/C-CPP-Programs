#include <iostream>

using namespace std;

int main()
{
    int row,col;
    cin>>row;
    cin>>col;
    
    int **arr= new int *[row];
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            arr[i]=new int[col];
        }
    }
    
    //input array 
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            cin>>arr[i][j];
        }
    }
    
    //Output array
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }

    return 0;
}
